#include<bits/stdc++.h>
using namespace std;
unordered_map<int,int> umap;
int main()
{

    unordered_map <int,int> umap;
    int n,a,k;
    cin>>n;
    for(int i=0;i<n-1;i++)
    {
        cin>>a;
        umap[a]=1;
    }
    for(int i=1;i<=n;i++)
    {
        if(umap[i]==0)
        {
            cout<<i<<endl;
            break;
        }    
    }

    
    
return 0;   
}
